		<dependency>
			<groupId>org.apache.activemq</groupId>
			<artifactId>activemq-all</artifactId>
			<version>${activemq.version}</version>
			<exclusions>
				<exclusion>
		         	<groupId>org.slf4j</groupId>
					<artifactId>jcl-over-slf4j</artifactId>
		        </exclusion>
			</exclusions>
		</dependency>
